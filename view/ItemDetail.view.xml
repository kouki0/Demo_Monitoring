			</form:SimpleForm>
		</content>
		<footer>
			<OverflowToolbar id="footerToolbar">
				<ToolbarSpacer id="footerToolbarSpacer"/>
				<Button id="assignButton" press="onAssignButtonPressed" text="{i18n&gt;xbut.assign}" type="Accept"/>
				<Button id="unassignButton" press="onUnassignButtonPressed" text="{i18n&gt;xbut.unassign}" type="Reject"/>
				<Button id="lockButton" press="" text="{i18n&gt;xbut.lock}" type="Default"/>
				<Button id="unlockButton" press="" text="{i18n&gt;xbut.unlock}" type="Transparent"/>
				<Button id="changeLSD" press="onLsdButtonPressed" text="{i18n&gt;xbut.lsd}" type="Transparent"/>
